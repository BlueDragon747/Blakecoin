CONFIG = asset_catalogs rez $$CONFIG

load(default_pre)

# Xcode developer path (osxcross cross-compilation â€” skip xcrun/xcodebuild checks)
QMAKE_XCODE_DEVELOPER_PATH = /opt/osxcross/target/SDK
QMAKE_XCODE_VERSION = 26.2

QMAKE_ASSET_CATALOGS_APP_ICON = AppIcon

